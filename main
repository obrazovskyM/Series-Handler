//В говнокоде и черт ногу сломит
//Тут даже компилятор плакать будет
/// //////////////////////////////////////////////////////////////////

#include "UI.h"
#include <execution> 

using namespace System;
using namespace System::Windows::Forms;

[STAThreadAttribute]

int main() {
	try {

		Application::EnableVisualStyles();
		Application::SetCompatibleTextRenderingDefault(false);
		
		SeriesHandler::UI ui;
		
		Application::Run(% ui);

	}
	catch (...) {
		MessageBox::Show("Initiolaze form error");
		return -1;
	}
	return 0;
}





